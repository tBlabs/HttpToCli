{
    "serverPort": 3000,
    "shell": "powershell",
    "routes": [
        {
            "url": "/ping",
            "command": "echo pong"
        },
        {
            "url": "/test/:test",
            "command": "echo test ok {test}"
        },
        {
            "url": "/:a/plus/:b",
            "command": "{a}+{b}"
        },
        {
            "url": "/shell/:cmd",
            "command": "{cmd}"
        },
        {
            "url": "/shell2/:cmd",
            "command": "{cmd}",
            "options": "Base64Params"
        },
        {
            "url": "/ListOfFilesContainingPackage",
            "command": "ls -l | grep \"package\""
        },
        {
            "url": "/ListOfFilesContainingFoo",
            "command": "ls -l | grep \"foo\""
        }
    ],
    "statics": [
        {
            "url": "/files",
            "dir": "./shared_files"
        }
    ]
}